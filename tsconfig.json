{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "rootDir": "src/",
    "noEmit": true,
    // "outDir": "build/gen"
  },
"references": [
  {
    "path": "gen"
  }
],
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "_archive",
    "build",
    "public",
  ]
}
